                <strong>{{ 'sections.cart.note_heading' | t }}  </strong>
              </div>
              <textarea
                class="seller-box-input"
                name="note"
                form="cart"
                id="Cart-note"
                placeholder="Special instructions for seller"
              >{{ cart.note }}</textarea>
            {%- endif -%}
            <ul class="list-unstyled cart-total-list">
              <li class="text-medium {% if cart.cart_level_discount_applications.size == 0 %}hidden{% endif %}" data-cart-original>
                <span>{{ 'sections.cart.subtotal' | t }} </span>
                <strong data-cart-original-price>{{ cart.original_total_price | money }}</strong>
              </li>
              {%- if cart.cart_level_discount_applications.size > 0 -%}
                {%- for discount in cart.cart_level_discount_applications -%}
                  <li class="text-medium cart-item-discount" data-cart-discount>
                    <span>{{ 'sections.cart.discount' | t }}['{{ discount.title }}'] </span>
                    <strong>-{{ discount.total_allocated_amount | money }}</strong>
                  </li>
                {%- endfor -%}
              {% endif %}
              <li class="cart-total-item text-large" data-cart-total>
                <p>{{ 'sections.cart.new_subtotal' | t }}</p>
                <strong class="h4 heading-font" data-cart-total-price>{{ cart.total_price | money }}</strong>
              </li>
              <li class="cart-total-item cart-item-total">
                <p class="checkout-text text-medium">
                  {%- if cart.taxes_included and shop.shipping_policy.body != blank -%}
                    {{ 'sections.cart.taxes_included_and_shipping_policy_html' | t: link: shop.shipping_policy.url }}
                  {%- elsif cart.taxes_included -%}
                    {{ 'sections.cart.taxes_included_but_shipping_at_checkout' | t }}
                  {%- elsif shop.shipping_policy.body != blank -%}
                    {{ 'sections.cart.taxes_and_shipping_policy_at_checkout_html' | t: link: shop.shipping_policy.url }}
                  {%- else -%}
                    {{ 'sections.cart.taxes_and_shipping_at_checkout' | t }}
                  {%- endif -%}
                </p>
              </li>
            </ul>            
        {% if settings.gift_wrap_product != blank
          and settings.gift_wrap_text != blank
          and settings.gift_wrap_product.available
          and giftWrapProductStatus == false
        %}
          <div class="giftcard-box">
            <label for="GiftWrapProduct" class="custom-checkbox">
              <input type="checkbox" id="GiftWrapProduct" name="giftCard" data-product="{{ settings.gift_wrap_product.first_available_variant.id }}" value="">
              <span class="checkmark"></span>
              {{ settings.gift_wrap_text }}
            </label>
          </div>
        {% endif %}
            <div class="cart-btn">
              <noscript>
                <button type="submit" id="cartUpdate" form="cart" class="button btn-block secondary-button"> {{ 'sections.cart.update' | t }}</button>
              </noscript>
              <button type="submit" name="checkout" id="cartCheckout" form="cart" class="button btn-block med-btn">
                <span>
                  <svg width="20" height="20" viewBox="0 0 20 20">
                    <path
                      d="M19.3908 7.525C18.9158 6.975 18.2241 6.66667 17.4991 6.66667H17.4241C17.0074 2.925 13.8241 0 9.97411 0C6.12411 0 2.93245 2.925 2.51578 6.66667H2.47411C1.74911 6.66667 1.05745 6.98333 0.582448 7.525C0.107448 8.075 -0.100886 8.8 -0.000885636 9.51667L0.882448 15.7C1.23245 18.15 3.35745 19.9917 5.83245 19.9917H14.1408C16.6158 19.9917 18.7408 18.15 19.0908 15.7L19.9741 9.51667C20.0741 8.8 19.8658 8.075 19.3908 7.525ZM9.97411 1.66667C12.9074 1.66667 15.3408 3.84167 15.7491 6.66667H4.19911C4.60745 3.84167 7.04078 1.66667 9.97411 1.66667ZM18.3241 9.28333L17.4408 15.4667C17.2074 17.1 15.7908 18.325 14.1408 18.325H5.83245C4.18245 18.325 2.76578 17.0917 2.53245 15.4667L1.64911 9.28333C1.61578 9.04167 1.68245 8.80833 1.84078 8.61667C1.99911 8.43333 2.22411 8.325 2.47411 8.325H17.4991C17.7408 8.325 17.9658 8.425 18.1241 8.60833C18.2824 8.79167 18.3574 9.03333 18.3158 9.275L18.3241 9.28333Z"
                      fill="currentColor" />
                  </svg>
                </span>  
                {{ 'sections.cart.checkout' | t }}
              </button>
              <div class="cart__dynamic-checkout-buttons additional-checkout-buttons">{{ content_for_additional_checkout_buttons }}</div>
            </div>       
          </div>
          {%- if section.settings.payment_enable -%}
            <div class="yv-payment-methods">              
              <ul class="list-unstyled list list-payment justify-content-center">
                {%- for type in shop.enabled_payment_types -%}
                  <li class="list-payment__item">{{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}</li>
                {%- endfor -%}
              </ul>
            </div>
          {%- endif -%}         
        </div>
      </div>
    </div>
  {% render 'cart-upsell-products' %}
  {% endif %}
</div>
<style>
  {% assign colorBrightness = settings.color_body_bg | color_brightness %}
  {%- if colorBrightness < 130 -%}
    #shipping_estimations_template.yv-shipping-estimations select{
      background: url({{ 'select-arrow-white.png' | asset_url }}) no-repeat 91% 52% /10px;
    }
  {%- else -%}
  #shipping_estimations_template.yv-shipping-estimations select{
    background:url({{ 'lang-dropdown-arrow.svg' | asset_url }}) no-repeat 91% 52% /10px;
  }
  {%- endif -%}
</style>
{% schema %}
{
  "name": "t:sections.main-cart.name",
  "tag": "section",
  "class": "cart-section",
  "settings": [
    {
      "type": "checkbox",
      "id": "show_cart_note",
      "default": true,
      "label": "t:sections.main-cart.settings.show_cart_note.label"
    },
    {
      "type": "checkbox",
      "id": "payment_enable",
      "default": true,
      "label": "t:sections.main-cart.settings.payment_enable.label"
    },
    {
      "type": "checkbox",
      "id": "shipping_estimate_enable",
      "default": true,
      "label": "t:sections.main-cart.settings.shipping_estimate_enable.label"
    },
    {
      "type": "select",
      "id": "seo_heading",
      "label": "t:common.header.heading_tag.label",
      "info": "t:common.header.heading_tag.info",
      "default": "h2",
      "options": [
        {
          "value": "h1",
          "label": "t:common.header.heading_tag.options.h1.label"
        },
        {
          "value": "h2",
          "label": "t:common.header.heading_tag.options.h2.label"
        },
        {
          "value": "h3",
          "label": "t:common.header.heading_tag.options.h3.label"
        },
        {
          "value": "h4",
          "label": "t:common.header.heading_tag.options.h4.label"
        },
        {
          "value": "h5",
          "label": "t:common.header.heading_tag.options.h5.label"
        },
        {
          "value": "h6",
          "label": "t:common.header.heading_tag.options.h6.label"
        }
      ]
    }
  ],
  "blocks": [
    {
      "type": "@app"
    }
  ]
}
{% endschema %}
